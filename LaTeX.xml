<templateSet group="LaTeX">
  <template name="if is defined" value="\ifdefined\$A$ $c1$\else $c2$\fi" description="" toReformat="true" toShortenFQNames="true">
    <variable name="A" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="c1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="c2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LATEX" value="true" />
      <option name="LATEX_MATH" value="false" />
    </context>
  </template>
  <template name="define" value="\newcommand{\$name$}{$value$}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="value" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
    <context>
      <option name="LATEX" value="true" />
      <option name="LATEX_MATH" value="false" />
    </context>
  </template>
</templateSet>